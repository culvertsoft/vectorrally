<Module>
	<Types>
	
		<Player>
			<name type="string">unnamed_player</name>
			<carColor type="CarColor"/>
		</Player>
	
		<PlayerCheckinRequest>
			<player type="Player" flags="required"/>
		</PlayerCheckinRequest>
		
		<PlayerCheckinReply>
			<player type="Player"/>
			<result type="bool"/>
			<failReason type="string"/>
		</PlayerCheckinReply>
	
		<WishList>
			<wishes type="list[Wish]">[]</wishes>
		</WishList>
		
		<PlayerList>
			<players type="list[Player]"/>
		</PlayerList>
		
		<PlayerJoinedMessage>
			<player type="Player"/>
		</PlayerJoinedMessage>
		
		<PlayerLeftMessage>
			<player type="Player"/>
		</PlayerLeftMessage>
		
		<ChatMessage>
			<text type="string"/>
		</ChatMessage>
		
	</Types>
</Module>
